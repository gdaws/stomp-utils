#!/usr/bin/env node

/*jslint node: true, indent: 2, unused: true, maxlen: 80, camelcase: true */

var nomnom = require('nomnom');

var commands = [
  'configure', 'send'
];

commands.forEach(function(command) {
  require('../lib/command/' + command).init(nomnom);
});

/*
process.on('uncaughtException', function(e) {
  process.exit(1);
});
*/

nomnom.parse();
